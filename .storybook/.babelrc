{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true,
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "theme-ui" }
    ],
    ["@babel/preset-typescript"]
  ],
  "plugins": ["@emotion"]
}
